            </Button>
            <Button disable="true" layoutX="48.0" layoutY="284.0" maxHeight="70.0"
              maxWidth="220.0" minHeight="70.0" minWidth="220.0" mnemonicParsing="false"
              onAction="#onListThreeButton" prefHeight="70.0" prefWidth="220.0"
              styleClass="primary-button" text="Button" fx:id="listThreeButton">
              <font>
                <Font name="System Bold" size="18.0"/>
              </font>
            </Button>
            <Button disable="true" layoutX="48.0" layoutY="384.0" maxHeight="70.0"
              maxWidth="220.0" minHeight="70.0" minWidth="220.0" mnemonicParsing="false"
              onAction="#onListFourButton" prefHeight="70.0" prefWidth="220.0"
