<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="780.0" maxWidth="1370.0" minHeight="780.0"
  minWidth="1370.0" prefHeight="780.0" prefWidth="1370.0"
  stylesheets="@../css/global.css" xmlns="http://javafx.com/javafx/21.0.1"
  fx:controller="seng202.team6.gui.ListScreenController">
  <children>
    <TabPane layoutX="332.0" prefHeight="780.0" prefWidth="1040.0" stylesheets="@../../../../build/resources/main/css/tabs_pane.css"
      tabClosingPolicy="UNAVAILABLE"
      tabMinHeight="50.0" fx:id="listScreenTabs">
      <tabs>
        <Tab text="VIEWING &lt;LIST_NAME&gt;" fx:id="tabViewing">
          <content>
            <VBox minHeight="720.0" prefHeight="720.0" prefWidth="1014.0">
