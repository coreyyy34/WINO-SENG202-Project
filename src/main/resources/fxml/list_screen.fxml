              <font>
                <Font name="System Bold" size="18.0"/>
              </font>
            </Button>
            <Button disable="true" layoutX="48.0" layoutY="484.0" maxHeight="70.0"
              maxWidth="220.0" minHeight="70.0" minWidth="220.0" mnemonicParsing="false"
              onAction="#onListFiveButton" prefHeight="70.0" prefWidth="220.0"
              styleClass="primary-button" text="Button" fx:id="listFiveButton">
              <font>
                <Font name="System Bold" size="18.0"/>
              </font>
            </Button>
            <Button layoutX="75.0" layoutY="644.0" mnemonicParsing="false"
              onAction="#onDeleteListRequestButton" prefHeight="33.0" prefWidth="167.0"
