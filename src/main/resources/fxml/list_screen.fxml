                        </Label>
                      </children>
                    </HBox>
                    <TableView GridPane.rowIndex="1" prefHeight="696.0" prefWidth="1040.0"
                      fx:id="tableView">
                      <columns>
                        <TableColumn minWidth="-1.0" prefWidth="-1.0"/>
                      </columns>
                    </TableView>
                  </children>
                  <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                  </columnConstraints>
                  <rowConstraints>
                    <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
