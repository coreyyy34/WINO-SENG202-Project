                      <children>
                        <Button layoutX="50.0" layoutY="254.0" mnemonicParsing="false"
                          onAction="#onBackButton" prefHeight="35.0" prefWidth="165.0"
                          styleClass="secondary-button" stylesheets="@../css/global.css"
                          text="Back" fx:id="backButton">
                          <font>
                            <Font name="System Bold" size="16.0"/>
                          </font>
                        </Button>
                        <Button layoutX="234.0" layoutY="254.0" mnemonicParsing="false"
                          onAction="#onCreateListConfirmButton" prefHeight="35.0" prefWidth="165.0"
                          styleClass="secondary-button" stylesheets="@../css/global.css"
                          text="Create List">
                          <font>
                            <Font name="System Bold" size="16.0"/>
                          </font>
                        </Button>
                        <Label layoutX="71.0" layoutY="128.0" text="Enter list Name">
                          <font>
                            <Font size="16.0"/>
                          </font>
                        </Label>
                        <Label layoutX="65.0" layoutY="208.0" prefHeight="16.0" prefWidth="159.0"
