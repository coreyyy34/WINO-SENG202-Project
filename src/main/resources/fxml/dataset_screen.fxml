<HBox xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seng202.team0.gui.DataTableController">
   <children>
      <VBox alignment="CENTER_LEFT">
         <children>
            <Button fx:id="uploadCSVButton" minHeight="50.0" minWidth="200.0" mnemonicParsing="false" onAction="#uploadCSVFile" prefHeight="50.0" prefWidth="200.0" text="Upload CSV">
