<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="780.0" maxWidth="1370.0" minHeight="780.0"
  minWidth="1370.0" prefHeight="780.0" prefWidth="1370.0"
  xmlns="http://javafx.com/javafx/21" fx:controller="seng202.team6.gui.DatasetImportController">
  <children>
    <Button minHeight="50.0" minWidth="200.0" mnemonicParsing="false" onAction="#importCSVFile"
      prefHeight="50.0" prefWidth="200.0" styleClass="secondary-button" stylesheets="@../css/global.css"
      text="Import CSV" fx:id="importCSVButton">
      <font>
        <Font name="System Bold" size="20.0"/>
      </font>
    </Button>
    <Button disable="true" layoutX="1053.0" layoutY="3.0" minHeight="30.0" minWidth="150.0"
      mnemonicParsing="false" onAction="#appendCSVFile" styleClass="secondary-button"
      stylesheets="@../css/global.css" text="Append" fx:id="appendButton">
      <font>
        <Font name="System Bold" size="20.0"/>
      </font>
    </Button>
    <Button disable="true" layoutX="1220.0" layoutY="3.0" minHeight="30.0" minWidth="150.0"
      mnemonicParsing="false" onAction="#replaceCSVFile" styleClass="secondary-button"
      stylesheets="@../css/global.css" text="Replace" fx:id="replaceButton">
      <font>
        <Font name="System Bold" size="20.0"/>
      </font>
    </Button>
    <ScrollPane style="-fx-background-color: white;" layoutY="64.0" prefHeight="716.0"
      prefWidth="1370.0">
      <content>
        <HBox spacing="15.0" fx:id="columnRemapList"/>
      </content>
    </ScrollPane>
  </children>
</AnchorPane>
